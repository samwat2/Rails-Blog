<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">User's</h1>
    <p class="lead font-italic">Future collaborators for the public -- contributors to the forum.</p>
  </div>
</div>
<div class="container">
	<div class="row">
		<% @users.each do |user| %>
			<div class="col-5 offset-.5">
				<div class="user names_of_users users-row">
				<%= image_tag "profile_picture_two.jpg", class: "rounded img-thumbnail mx-auto d-block" %>
					<h3><%= user.first_name %> <%= user.last_name %></h3>
					<%= link_to "User Page", user %>
				</div>
		  </div>
		<% end %>
	</div>
</div>
